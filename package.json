{
  "name": "i18n-app-translator",
  "version": "1.0.0",
  "description": "AI-powered translation tool for i18n files with vector database support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "i18n-app-translator": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "API.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "spell:check": "cspell 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "keywords": [
    "i18n",
    "translation",
    "localization",
    "internationalization",
    "ai",
    "openai",
    "vector-database",
    "weaviate",
    "pinecone",
    "cli"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/i18n-app-translator.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/i18n-app-translator/issues"
  },
  "homepage": "https://github.com/yourusername/i18n-app-translator#readme",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "cspell": "^8.17.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "@langchain/openai": "^0.4.4",
    "@pinecone-database/pinecone": "^5.0.2",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "langchain": "^0.3.19",
    "openai": "^4.85.4",
    "pinecone-client": "^2.0.0",
    "weaviate-ts-client": "^2.2.0"
  }
}
